  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:contracts && npm run build:backend && npm run build:frontend",
    "build:contracts": "cd contracts && npx hardhat compile",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "test": "npm run test:contracts && npm run test:backend && npm run test:frontend",
    "test:contracts": "cd contracts && npx hardhat test",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:e2e": "cd tests && npm run test:e2e",
    "lint": "npm run lint:contracts && npm run lint:backend && npm run lint:frontend",
    "lint:contracts": "cd contracts && npx solhint 'contracts/**/*.sol'",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:fix": "npm run lint:contracts -- --fix && npm run lint:backend -- --fix && npm run lint:frontend -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,sol,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,sol,json,md}\"",
    "deploy:localhost": "cd contracts && npx hardhat run scripts/deploy.ts --network localhost",
    "deploy:testnet": "cd contracts && npx hardhat run scripts/deploy.ts --network goerli",
    "deploy:mainnet": "cd contracts && npx hardhat run scripts/deploy.ts --network mainnet",
    "verify:testnet": "cd contracts && npx hardhat verify --network goerli",
    "verify:mainnet": "cd contracts && npx hardhat verify --network mainnet",
    "node:local": "cd contracts && npx hardhat node",
    "console:testnet": "cd contracts && npx hardhat console --network goerli",
    "console:mainnet": "cd contracts && npx hardhat console --network mainnet",
    "security:slither": "cd contracts && slither .",
    "security:mythril": "cd contracts && myth analyze contracts/**/*.sol",
    "security:audit": "npm run security:slither && npm run security:mythril",
    "docs:generate": "npx typedoc",
    "docs:contracts": "cd contracts && npx hardhat docgen",
    "docs:serve": "npx http-server docs",
    "clean": "npm run clean:contracts && npm run clean:backend && npm run clean:frontend",
    "clean:contracts": "cd contracts && npx hardhat clean",
    "clean:backend": "cd backend && npm run clean",
    "clean:frontend": "cd frontend && npm run clean",
    "install:all": "npm install && npm run install:contracts && npm run install:backend && npm run install:frontend",
    "install:contracts": "cd contracts && npm install",
    "install:backend": "cd backend && npm install", 
    "install:frontend": "cd frontend && npm install",
    "docker:build": "docker build -t ubiquitous-liquidity-platform .",
    "docker:run": "docker run -p 3000:3000 -p 8545:8545 ubiquitous-liquidity-platform",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "compliance:nist": "node scripts/compliance/nist-check.js",
    "compliance:cmmc": "node scripts/compliance/cmmc-audit.js",
    "compliance:soc2": "node scripts/compliance/soc2-report.js",
    "federal:sam-sync": "node scripts/federal/sam-sync.js",
    "federal:gsa-update": "node scripts/federal/gsa-update.js",
    "rhodium:price-update": "node scripts/rhodium/price-oracle.js",
    "sacred-geometry:optimize": "node scripts/sacred-geometry/golden-ratio-calc.js",
    "natural-resources:update": "node scripts/natural-resources/valuation-update.js"
  },

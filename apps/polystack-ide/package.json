{
  "name": "@gtek/polystack-ide",
  "version": "1.0.0",
    "@gtek/liquidnomocs-framework": "^1.0.0", "description": "AI-native fullstack development environment with Samsung exclusive tools",
  "main": "dist/main.js",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "concurrently \"npm run build:watch\" \"npm run electron:dev\"",
    "electron": "electron .",
    "electron:dev": "wait-on dist/main.js && electron . --dev",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --mode production",
    "build:watch": "concurrently \"npm run build:main:watch\" \"npm run build:renderer:watch\"",
    "build:main:watch": "tsc -p tsconfig.main.json --watch",
    "build:renderer:watch": "webpack --mode development --watch",
    "dist": "npm run build && electron-builder",
    "dist:dir": "npm run build && electron-builder --dir",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "build": {
    "appId": "com.gtek.polystack-ide",
    "productName": "PolyStack IDE",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "@monaco-editor/react": "^4.5.1",
    "@types/node": "^20.0.0",
    "electron-store": "^8.1.0",
    "monaco-editor": "^0.41.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "styled-components": "^6.0.7",
    "web3": "^4.0.0",
    "@gtek/liquidnomocs-framework": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "electron": "^25.3.1",
    "electron-builder": "^24.6.3",
    "eslint": "^8.45.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "electron",
    "ai",
    "ide",
    "fullstack",
    "samsung",
    "polystack",
    "ancient-wisdom",
    "code-generation"
  ],
  "author": "Jerome Elston Hill Jr.",
  "license": "MIT",
  "gtek": {
    "ironRule": "Commission or charitable donation only",
    "samsungExclusive": true,
    "aiNative": true,
    "ancientWisdom": "Polymath Vizier protocols"
  }
}